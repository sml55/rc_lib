#!/bin/bash

zypper -n install zsh 


mkdir ~/src
cd ~/src

#rc_lib
git clobe https://github.com/twist/rc_lib.git ./rc_lib
cd rc_lib && ./setup.sh && cd -

#FZF
git clone https://github.com/junegunn/fzf.git ~/.fzf
~/.fzf/install



